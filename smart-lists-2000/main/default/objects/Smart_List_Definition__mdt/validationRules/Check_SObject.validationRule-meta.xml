<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_SObject</fullName>
    <active>true</active>
    <errorConditionFormula>(ISPICKVAL(Data_Source_Type__c , &quot;SOQL with Sharing&quot;) ||ISPICKVAL(Data_Source_Type__c , &quot;SOQL without Sharing&quot;)) &amp;&amp;
ISBLANK(SObject__c)</errorConditionFormula>
    <errorDisplayField>SObject__c</errorDisplayField>
    <errorMessage>SObject is required for types &apos;SOQL with/without Sharing&apos;</errorMessage>
</ValidationRule>
