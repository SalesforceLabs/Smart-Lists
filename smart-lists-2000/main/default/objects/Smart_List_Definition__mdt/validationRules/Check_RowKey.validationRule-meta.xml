<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_RowKey</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL(Data_Source_Type__c, &quot;Apex Data Source&quot;) &amp;&amp;
ISBLANK( Row_Key__c )</errorConditionFormula>
    <errorDisplayField>Row_Key__c</errorDisplayField>
    <errorMessage>Row Key is required for type &apos; Apex Data Source&apos;</errorMessage>
</ValidationRule>
